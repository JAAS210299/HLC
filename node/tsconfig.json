{
  "compilerOptions": {
    "target": "es2017",
    "module": "ES2022",
    "outDir": "./js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true 
  },
  "include":  [
    "js/ts/**/*",
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
